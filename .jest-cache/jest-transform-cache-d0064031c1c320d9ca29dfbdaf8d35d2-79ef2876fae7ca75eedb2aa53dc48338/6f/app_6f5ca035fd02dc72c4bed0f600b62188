"use strict";

// This file configures the Express application, sets up middleware for parsing requests, and defines API routes.

const express = require('express');
const cors = require('cors');
const helmet = require('helmet');
const morgan = require('morgan');
const multer = require('multer');
const path = require('path');
const requestLogger = require('./middleware/requestLogger');
const errorHandler = require('./middleware/errorHandler');
const docsRouter = require('./routes/docs');
const analysisRouter = require('./routes/analysisRoutes');
const prometheusMiddleware = require('express-prometheus-middleware');
const responseTime = require('response-time');
const monitoringRoutes = require('./routes/monitoringRoutes');
const {
  sanitizeRequest
} = require('./middleware/sanitizer');
const rateLimiter = require('./middleware/rateLimiter');
const cacheService = require('./services/cacheService');
const healthCheck = require('./middleware/healthCheck');
const statementRoutes = require('./routes/statementRoutes');
const swaggerUI = require('swagger-ui-express');
const swaggerSpec = require('./config/swagger');
const app = express();
const upload = multer({
  limits: {
    fileSize: 5 * 1024 * 1024 // 5MB limit
  },
  fileFilter: (req, file, cb) => {
    if (path.extname(file.originalname) !== '.pdf') {
      return cb(new Error('Only PDF files are allowed'));
    }
    cb(null, true);
  }
});

// Security middleware
app.use(helmet());
app.use(cors({
  origin: process.env.ALLOWED_ORIGINS?.split(',') || '*',
  methods: ['GET', 'POST'],
  allowedHeaders: ['Content-Type', 'Authorization']
}));
app.use(rateLimiter.middleware());

// Request parsing & compression
app.use(express.json({
  limit: '1mb'
}));
app.use(express.urlencoded({
  extended: true,
  limit: '1mb'
}));
app.use(sanitizeRequest);
app.use(compression());

// Logging and monitoring
app.use(requestLogger);
app.use(responseTime());
app.use(prometheusMiddleware({
  metricsPath: '/metrics',
  collectDefaultMetrics: true,
  requestDurationBuckets: [0.1, 0.5, 1, 2, 5]
}));
app.use(morgan('dev'));

// Health and monitoring routes
app.get('/health', healthCheck.middleware());
app.use('/monitoring', monitoringRoutes);

// API Documentation
app.use('/api-docs', swaggerUI.serve, swaggerUI.setup(swaggerSpec));

// API Routes with file upload
app.use('/api/analysis', upload.single('bankStatement'), analysisRouter);
app.use('/api/statements', statementRoutes);

// Error handling
app.use((err, req, res, next) => {
  console.error(err.stack);
  res.status(500).json({
    success: false,
    error: 'Internal Server Error'
  });
});

// Create uploads directory if it doesn't exist
const uploadsDir = path.join(__dirname, '../uploads');
require('fs').mkdirSync(uploadsDir, {
  recursive: true
});

// Initialize cache cleanup
cacheService.startCleanup();
module.exports = app;
//# sourceMappingURL=data:application/json;charset=utf-8;base64,